{
  "expo": {
    "name": "Ã©ist",
    "slug": "eist-app",
    "version": "3.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "eistapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oootini.eistapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "audio"
        ],
        "NSPhotoLibraryUsageDescription": "We save app screenshots to your photo library so that you can access them later.",
        "NSMicrophoneUsageDescription": "This app streams audio and does not record you, but a library we use requires microphone permissions to function."
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK"
      ],
      "package": "com.oootini.eistapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "backgroundColor": "#4733FF",
      "dark": {
        "backgroundColor": "#4733FF"
      }
    },
    "plugins": [
      "expo-audio",
      "expo-video",
      "expo-router",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4f034ae2-70e3-4215-8782-3aec98781aa6"
      }
    },
    "owner": "oootini"
  }
}
